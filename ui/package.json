{
  "name": "ecms-ui",
  "version": "1.0.0",
  "description": "UI for ecms",
  "main": "dist/index.ts",
  "scripts": {
    "dev": "chmod +x ./configure-runtime-env.sh && ./configure-runtime-env.sh && mv ./runtime-env.js public && webpack serve --port 3000",
    "build": "rm -rf ./dist && mkdir ./dist && touch dist/runtime-env.js && NODE_ENV=production webpack",
    "get-stats": "rm -rf ./dist && mkdir ./dist && NODE_ENV=production webpack --profile --json > dist/stats.json",
    "analyse-stats": "webpack-bundle-analyzer dist/stats.json",
    "docker-build": "docker build -t ecms-ui .",
    "docker-run": "docker run --net=host --env-file=.env ecms-ui",
    "lint": "eslint . --ext .ts --ext .tsx",
    "lint-fix": "eslint . --fix --ext .ts --ext .tsx",
    "prettier": "prettier -c src",
    "prettier-fix": "prettier --write src",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build"
  },
  "author": "C Elston",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@storybook/addon-docs": "^10.1.2",
    "@storybook/addon-styling-webpack": "^3.0.0",
    "@storybook/addon-webpack5-compiler-swc": "^4.0.2",
    "@storybook/react-webpack5": "^10.1.2",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/codemirror": "^5.60.9",
    "@types/jest": "^29.2.0",
    "@types/loader-utils": "^3.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.5",
    "@types/react-router": "^5.1.19",
    "@types/react-router-dom": "^5.3.3",
    "css-loader": "^6.7.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-storybook": "^10.1.2",
    "globals": "^16.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.3.1",
    "loader-utils": "^3.3.1",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss-loader": "^7.0.1",
    "prettier": "3.5.3",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "storybook": "^10.1.2",
    "style-loader": "^3.3.1",
    "ts-jest": "^29.2.6",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "typescript-plugin-css-modules": "^5.2.0",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@blueprintjs/core": "^5.17.6",
    "@blueprintjs/select": "^5.3.18",
    "@codemirror/lang-markdown": "^6.2.0",
    "@codemirror/language-data": "^6.3.1",
    "@codemirror/view": "^6.17.1",
    "@tanstack/react-query": "^5.68.0",
    "@uiw/react-codemirror": "^4.21.13",
    "axios": "^1.2.3",
    "classnames": "^2.5.1",
    "modern-normalize": "^3.0.1",
    "react": "^18.3.0",
    "react-aria-components": "^1.10.1",
    "react-dom": "^18.3.0",
    "react-error-boundary": "^5.0.0",
    "react-icons": "5.5.0",
    "react-markdown": "^8.0.7",
    "react-resize-detector": "^7.1.2",
    "react-responsive": "^10.0.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "rehype-highlight": "^6.0.0",
    "remark-definition-list": "^1.2.0",
    "remark-emoji": "^3.0.2",
    "remark-gfm": "^3.0.1",
    "remark-smartypants": "^2.0.0",
    "yaml": "^2.7.0"
  }
}
